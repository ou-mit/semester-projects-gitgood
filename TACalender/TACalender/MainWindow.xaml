<Window x:Class="TACalender.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TACalender"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="HomeText" HorizontalAlignment="Center" Margin="0,28,0,0" TextWrapping="Wrap" Text="Choose one of the following" VerticalAlignment="Top" Height="53" Width="300" FontSize="22"/>
        <Button x:Name="btnHomeAdmin" Content="Administrator" HorizontalAlignment="Left" Margin="137,190,0,0" VerticalAlignment="Top" Height="85" Width="173" FontSize="22" Click="btnHomeAdmin_Click"/>
        <Button x:Name="btnHomeTA" Content="Teaching Assistant" HorizontalAlignment="Left" Margin="458,190,0,0" VerticalAlignment="Top" Height="85" Width="192" FontSize="22" Click="btnHomeTA_Click"/>

    </Grid>
</Window>
